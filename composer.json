{
    "name": "readly24/yii2-air-datepicker",
    "description": "Air datepicker",
    "type": "yii2-extension",
    "keywords": ["yii2","extension air-datepicker datepicker air"],
    "license": "MIT",
    "authors": [
        {
            "name": "readly24",
            "email": "readly@list.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "bower-asset/air-datepicker": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "readly24\\airdatepicker\\": "src"
        }
    },
    "support": {
        "issues": "https://github.com/readly24/yii2-air-datepicker/issues?state=open",
        "source": "https://github.com/readly24/yii2-air-datepicker"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
